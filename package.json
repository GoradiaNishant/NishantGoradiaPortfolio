{
  "name": "portfolio-website",
  "version": "1.0.0",
  "description": "Portfolio website with image proxy server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'Build completed - static site ready for GitHub Pages'",
    "deploy": "npm run deploy:firebase",
    "deploy:firebase": "firebase deploy",
    "cache:stats": "curl http://localhost:3000/cache/stats",
    "cache:clear": "curl -X DELETE http://localhost:3000/cache/clear"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "firebase-tools": "^13.0.0",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "portfolio",
    "image-proxy",
    "firebase",
    "express"
  ],
  "author": "Nishant Goradia",
  "license": "MIT"
}
